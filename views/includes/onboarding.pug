doctype html5
html(lang='en')
  block vars
  head
    title PASchedules: #{path}
    block styles
    link(rel='stylesheet', href='/static/assets/styles/onboarding.css')
    meta(name='viewport' content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no')
  body
    div.wrapper
      main
        h1 PASchedules
        block form
    include analytics.pug
    script(type='text/javascript', src='//cdn.socket.io/socket.io-1.4.5.js')
    script(type='text/javascript', src='//cdn.rawgit.com/feathersjs/feathers-client/v1.1.0/dist/feathers.js')
    script(type='text/javascript', src='https://ajax.googleapis.com/ajax/libs/jquery/3.0.0/jquery.min.js')
    script(type='text/javascript', src='/static/js/validate.js')

    block scripts
